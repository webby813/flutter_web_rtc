# Firebase Configuration
# Copy this file to .env and fill in your Firebase project values
# Get these values from Firebase Console -> Project Settings -> Your apps

# Web Configuration
FIREBASE_WEB_API_KEY=your_web_api_key_here
FIREBASE_WEB_APP_ID=your_web_app_id_here
FIREBASE_WEB_MESSAGING_SENDER_ID=your_messaging_sender_id_here
FIREBASE_WEB_PROJECT_ID=your_project_id_here
FIREBASE_WEB_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_WEB_STORAGE_BUCKET=your_project_id.firebasestorage.app
FIREBASE_WEB_MEASUREMENT_ID=your_measurement_id_here

# Android Configuration
FIREBASE_ANDROID_API_KEY=your_android_api_key_here
FIREBASE_ANDROID_APP_ID=your_android_app_id_here

# iOS Configuration
FIREBASE_IOS_API_KEY=your_ios_api_key_here
FIREBASE_IOS_APP_ID=your_ios_app_id_here
FIREBASE_IOS_BUNDLE_ID=com.example.yourapp

# macOS Configuration (usually same as iOS)
FIREBASE_MACOS_API_KEY=your_macos_api_key_here
FIREBASE_MACOS_APP_ID=your_macos_app_id_here
FIREBASE_MACOS_BUNDLE_ID=com.example.yourapp

# Windows Configuration
FIREBASE_WINDOWS_API_KEY=your_windows_api_key_here
FIREBASE_WINDOWS_APP_ID=your_windows_app_id_here
FIREBASE_WINDOWS_MEASUREMENT_ID=your_measurement_id_here
